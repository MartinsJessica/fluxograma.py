numero = int(input("Digite um numero"))

if numero%3 == 0 and numero %5 == 0:
    print("é divisível por três e cinco")
elif numero%3 == 0:
    print("é só divísivel por três")
elif numero%5 ==0:
    print("é só divisível por cinco")
else:
    print("O número não é divisível por três e nem por cinco!")
